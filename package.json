{
  "name": "nhl-stats",
  "version": "1.0.0",
  "scripts": {
    "start": "cross-env NODE_ENV=production CF_ENV=production RULES_ENV=production node ./bin/www.js",
    "start:np": "cross-env NODE_ENV=production CF_ENV=nonProd RULES_ENV=nonProd node ./bin/www.js",
    "local": "cross-env NODE_ENV=local CF_ENV=local RULES_ENV=local node ./bin/www.js",
    "local:bundle": "NODE_ENV=production CF_ENV=local RULES_ENV=local TRICK_VCAP=true node ./bin/www.js",
    "clean": "rimraf public/scripts/bundle.js public/scripts/bundle.js.map coverage",
    "package": "cross-env NODE_ENV=production node ./node_modules/webpack/bin/webpack --mode production",
    "lint": "cross-env eslint src",
    "lint-fix": "node ./node_modules/eslint/bin/eslint --ext .js ./src --fix",
    "lint-test": "cross-env eslint test",
    "test": "node ./node_modules/mocha/bin/mocha test",
    "test:log": "mocha --timeout 50000 -R json-stream test > mocha.log",
    "test-cov": "clear && cross-env NODE_ENV=test node ./node_modules/nyc/bin/nyc ./node_modules/mocha/bin/mocha test && open coverage/lcov-report/index.html",
    "test-cov-sum": "cross-env NODE_ENV=test node ./node_modules/nyc/bin/nyc ./node_modules/mocha/bin/mocha test && node ./node_modules/nyc/bin/nyc report --reporter=json-summary"
  },
  "files": [
    "public",
    "bin",
    "server.js",
  ],
  "description": "",
  "dependencies": {
    "axios": "~0.18.0",
    "compression": "^1.7.3",
    "cross-env": "~5.2.0",
    "diskspace": "~2.0.0",
    "es6-promise": "~4.2.5",
    "express": "^4.16.3",
    "flux": "^3.1.3",
    "immutable": "^4.0.0-rc.12",
    "lodash": "~4.17.11",
    "moment": "^2.22.2",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.1",
    "react-router-dom": "^4.3.1",
    "react-select": "^2.0.0",
    "uuid": "~3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "~7.0.0-rc.3",
    "@babel/core": "~7.0.0-rc.3",
    "@babel/plugin-proposal-object-rest-spread": "~7.0.0-beta.55",
    "@babel/polyfill": "~7.0.0-beta.55",
    "@babel/preset-env": "~7.0.0-beta.55",
    "@babel/preset-react": "~7.0.0-beta.55",
    "@babel/register": "~7.0.0-beta.55",
    "babel-eslint": "~8.2.6",
    "babel-loader": "~8.0.0-beta.4",
    "babel-plugin-autobind-class-methods": "~5.0.1",
    "babel-plugin-istanbul": "~4.1.6",
    "babel-plugin-transform-class-properties": "~6.24.1",
    "css-loader": "~1.0.0",
    "eslint": "~5.2.0",
    "eslint-config-airbnb": "~17.0.0",
    "eslint-plugin-import": "~2.13.0",
    "eslint-plugin-jsx-a11y": "~6.1.1",
    "eslint-plugin-react": "~7.10.0",
    "jsdom": "~11.12.0",
    "json-loader": "~0.5.7",
    "mini-css-extract-plugin": "~0.4.1",
    "node-sass": "~4.9.2",
    "piping": "~1.0.0-rc.4",
    "prettier": "~1.14.3",
    "react-hot-loader": "~3.1.3",
    "rimraf": "~2.6.2",
    "sass-loader": "~7.0.3",
    "style-loader": "~0.21.0",
    "uglifyjs-webpack-plugin": "~1.2.7",
    "webpack": "~4.20.2",
    "webpack-command": "~0.4.1",
    "webpack-dev-middleware": "~3.1.3",
    "webpack-hot-middleware": "~2.22.3"
  },
  "engines": {
    "node": "^10.x"
  }
}
