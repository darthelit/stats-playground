{
  "name": "nhl-stats",
  "version": "1.0.0",
  "scripts": {
    "start": "cross-env NODE_ENV=production node ./bin/www.js",
    "start:np": "cross-env NODE_ENV=production node ./bin/www.js",
    "local": "cross-env NODE_ENV=local node ./bin/www.js",
    "local:bundle": "NODE_ENV=production node ./bin/www.js",
    "clean": "rimraf public/scripts/bundle.js public/scripts/bundle.js.map coverage",
    "package": "cross-env NODE_ENV=production node ./node_modules/webpack/bin/webpack --mode production",
    "lint": "cross-env eslint src",
    "lint-fix": "node ./node_modules/eslint/bin/eslint --ext .js ./src --fix",
    "lint-test": "cross-env eslint test",
    "test": "node ./node_modules/mocha/bin/mocha test",
    "test:log": "mocha --timeout 50000 -R json-stream test > mocha.log",
    "test-cov": "clear && cross-env NODE_ENV=test node ./node_modules/nyc/bin/nyc ./node_modules/mocha/bin/mocha test && open coverage/lcov-report/index.html",
    "test-cov-sum": "cross-env NODE_ENV=test node ./node_modules/nyc/bin/nyc ./node_modules/mocha/bin/mocha test && node ./node_modules/nyc/bin/nyc report --reporter=json-summary"
  },
  "files": [
    "public",
    "bin",
    "server.js"
  ],
  "description": "",
  "dependencies": {
    "axios": "~0.19.0",
    "baseui": "^9.52.0",
    "compression": "^1.7.4",
    "cross-env": "~6.0.0",
    "diskspace": "~2.0.0",
    "es6-promise": "~4.2.8",
    "express": "~4.17.1",
    "flux": "^3.1.3",
    "immutable": "^4.0.0-rc.12",
    "lodash": "~4.17.15",
    "moment": "^2.24.0",
    "office-ui-fabric-react": "^7.89.1",
    "prop-types": "^15.7.2",
    "react": "~16.9.0",
    "react-dom": "~16.9.0",
    "react-router-dom": "^5.1.0",
    "react-select": "~3.0.5",
    "styletron-engine-atomic": "^1.4.5",
    "styletron-react": "^5.2.7",
    "uuid": "~3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "~7.6.2",
    "@babel/core": "~7.6.2",
    "@babel/plugin-proposal-object-rest-spread": "~7.6.2",
    "@babel/polyfill": "~7.6.0",
    "@babel/preset-env": "~7.6.2",
    "@babel/preset-react": "~7.0.0",
    "@babel/register": "~7.6.2",
    "babel-eslint": "~10.0.3",
    "babel-loader": "~8.0.6",
    "babel-plugin-autobind-class-methods": "~5.0.1",
    "babel-plugin-istanbul": "~5.2.0",
    "babel-plugin-transform-class-properties": "~6.24.1",
    "css-loader": "~3.2.0",
    "eslint": "~6.4.0",
    "eslint-config-airbnb": "~18.0.1",
    "eslint-plugin-baseui": "^9.52.0",
    "eslint-plugin-import": "~2.18.2",
    "eslint-plugin-jsx-a11y": "~6.2.3",
    "eslint-plugin-react": "~7.14.3",
    "jsdom": "~15.1.1",
    "json-loader": "~0.5.7",
    "mini-css-extract-plugin": "~0.8.0",
    "node-sass": "~4.13.0",
    "piping": "~1.0.0-rc.4",
    "prettier": "~1.18.2",
    "react-hot-loader": "~4.12.14",
    "rimraf": "~3.0.0",
    "sass-loader": "~8.0.0",
    "style-loader": "~1.0.0",
    "uglifyjs-webpack-plugin": "~2.2.0",
    "webpack": "~4.41.0",
